# Testnet profile (MVP demo)
DEPLOY_ENV=testnet
RPC_URL=https://sepolia.infura.io/v3/YOUR_KEY
CHAIN_ID=11155111

# Set to the testnet PoolManager used for your Uniswap v4 deployment
POOL_MANAGER_ADDRESS=0x0000000000000000000000000000000000000001

# Fast testnet path: private key is allowed for speed
DEPLOYER_PRIVATE_KEY=0xYOUR_TESTNET_PRIVATE_KEY

# Optional: transfer ownership immediately after deployment
# (recommended to move to a custody account even on testnet demos)
HOOK_OWNER=

DEPLOY_USE_LEDGER=false
DEPLOY_SENDER=

ENABLE_VERIFY=true
ETHERSCAN_API_KEY=YOUR_EXPLORER_API_KEY
ALLOW_MAINNET_PRIVATE_KEY=false

# Optional smoke check overrides
HOOK_ADDRESS=
SMOKE_PRIVATE_KEY=
UPDATER_ADDRESS=
