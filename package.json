{
  "name": "jack-monorepo",
  "version": "0.1.15",
  "private": true,
  "scripts": {
    "build": "vite build --config apps/landing/vite.config.ts",
    "build:turbo": "turbo run build",
    "dev": "pnpm dev:landing",
    "dev:landing": "vite --config apps/landing/vite.config.ts --host 0.0.0.0 --port 3000",
    "dev:dashboard": "cd apps/dashboard && pnpm dev",
    "dev:all": "concurrently \"pnpm dev:landing\" \"pnpm dev:dashboard\"",
    "lint": "turbo run lint",
    "release": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "start": "serve -s apps/landing/dist -l 8080",
    "prepare": "husky || true",
    "agent:run": "tsx scripts/agent-runner.ts",
    "agent:dashboard": "tsx scripts/agent-dashboard.ts",
    "agent:sync": "tsx scripts/sync-github-tasks.ts",
    "agent:verify-gh": "tsx scripts/verify-gh-integration.ts",
    "agent:tracker": "tsx scripts/github-project-tracker.ts"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.5",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@edcalderon/versioning": "^1.0.11",
    "@secretlint/quick-start": "^11.3.1",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.1",
    "@types/blessed": "^0.1.27",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.3.0",
    "blessed": "^0.1.81",
    "concurrently": "^8.2.2",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.0.0",
    "secretlint": "11.3.1",
    "tsx": "^4.21.0",
    "turbo": "latest",
    "dotenv": "^16.4.5",
    "typescript": "^5.0.0",
    "vite": "^5.4.0"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": "22.22.0"
  },
  "npmClient": "pnpm",
  "tempo": {
    "installCommand": "npm install",
    "installWorkingDirectory": "npm-shim",
    "devCommand": "npm run dev",
    "devWorkingDirectory": "npm-shim",
    "useNpmShim": true,
    "npmShimWorkingDirectory": "npm-shim",
    "packageManager": "pnpm",
    "appWorkingDirectory": "npm-shim"
  },
  "lint-staged": {
    "*": "secretlint"
  }
}
