{
  "name": "jack-monorepo",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:turbo": "turbo run build",
    "dev": "vite --host 0.0.0.0 --port 3001",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "start": "serve -s dist -l 8080",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.5",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@secretlint/quick-start": "^11.3.1",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.0.0",
    "secretlint": "^11.3.1",
    "turbo": "latest",
    "typescript": "^5.0.0",
    "vite": "^5.4.0"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18.0.0"
  },
  "npmClient": "pnpm",
  "tempo": {
    "installCommand": "npm install",
    "installWorkingDirectory": "npm-shim",
    "devCommand": "npm run dev",
    "devWorkingDirectory": "npm-shim",
    "useNpmShim": true,
    "npmShimWorkingDirectory": "npm-shim",
    "packageManager": "pnpm",
    "appWorkingDirectory": "npm-shim"
  },
  "lint-staged": {
    "*": "secretlint"
  }
}