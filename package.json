{
  "name": "jack-monorepo",
  "version": "0.1.59",
  "private": true,
  "scripts": {
    "build": "vite build --config apps/landing/vite.config.ts",
    "build:turbo": "turbo run build",
    "dev": "pnpm dev:landing",
    "dev:landing": "vite --config apps/landing/vite.config.ts --host 0.0.0.0 --port 3000",
    "dev:dashboard": "cd apps/dashboard && pnpm dev",
    "dev:docs": "cd apps/docs && pnpm dev",
    "dev:videos": "cd apps/videos && pnpm dev",
    "dev:video-recorder": "cd apps/video-recorder && pnpm dev",
    "dev:video-recorder:preview": "cd apps/video-recorder && pnpm dev:preview",
    "dev:all": "concurrently \"pnpm dev:landing\" \"pnpm dev:dashboard\" \"pnpm dev:docs\"",
    "lint": "turbo run lint",
    "release": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:all": "node scripts/release.js patch --with-docs-deploy",
    "release:all:minor": "node scripts/release.js minor --with-docs-deploy",
    "release:all:major": "node scripts/release.js major --with-docs-deploy",
    "release:docs": "node scripts/release-docs.js",
    "release:docs:deploy": "node scripts/release-docs.js --trigger-deploy --ref main",
    "whitepaper:build": "node scripts/whitepaper/build.js --all",
    "whitepaper:validate": "node scripts/whitepaper/validate.js",
    "docs:impact:check": "node scripts/check-doc-impact.js",
    "version:check": "node scripts/check-version-sync.js",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "start": "serve -s apps/landing/dist -l 8080",
    "prepare": "husky || true",
    "agent:run": "node --import tsx scripts/agent-runner.ts",
    "agent:dashboard": "node --import tsx scripts/agent-dashboard.ts",
    "agent:sync": "node --import tsx scripts/sync-github-tasks.ts",
    "agent:verify-gh": "node --import tsx scripts/verify-gh-integration.ts",
    "agent:tracker": "node --import tsx scripts/github-project-tracker.ts",
    "contracts:build": "./scripts/contracts/forge.sh build",
    "contracts:test": "./scripts/contracts/forge.sh test -vv",
    "contracts:deploy:testnet": "./scripts/contracts/deploy-hook.sh contracts/.env.testnet",
    "contracts:deploy:mainnet": "./scripts/contracts/deploy-hook.sh contracts/.env.mainnet",
    "yellow:demo": "node --import tsx scripts/prize-tracks/yellow-testnet-demo.ts",
    "contracts:smoke:testnet": "./scripts/contracts/smoke-hook.sh contracts/.env.testnet",
    "clean:docs": "node --import tsx scripts/clean-docs.ts",
    "upload:assets": "node --import tsx scripts/upload-assets.ts",
    "upload:assets:testnet": "node --import tsx scripts/upload-assets.ts --env=testnet",
    "upload:assets:production": "node --import tsx scripts/upload-assets.ts --env=production",
    "setup:bucket": "bash scripts/setup-asset-bucket.sh",
    "setup:bucket:testnet": "bash scripts/setup-asset-bucket.sh testnet",
    "setup:bucket:production": "bash scripts/setup-asset-bucket.sh production"
  },
  "dependencies": {
    "@erc7824/nitrolite": "^0.5.3",
    "@metamask/sdk": "^0.34.0",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@tanstack/react-query": "^5.90.20",
    "@types/ws": "^8.18.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.5",
    "three": "^0.182.0",
    "viem": "2.x",
    "wagmi": "^3.4.2",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@edcalderon/versioning": "^1.0.11",
    "@google-cloud/storage": "^7.19.0",
    "@secretlint/quick-start": "^11.3.1",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.1",
    "@types/blessed": "^0.1.27",
    "@types/eslint-plugin-security": "^3.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.3.0",
    "blessed": "^0.1.81",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.0.0",
    "secretlint": "11.3.1",
    "tsx": "^4.21.0",
    "turbo": "latest",
    "typescript": "^5.0.0",
    "vite": "^5.4.0"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": "22.22.0"
  },
  "npmClient": "pnpm",
  "tempo": {
    "installCommand": "npm install",
    "installWorkingDirectory": "npm-shim",
    "devCommand": "npm run dev",
    "devWorkingDirectory": "npm-shim",
    "useNpmShim": true,
    "npmShimWorkingDirectory": "npm-shim",
    "packageManager": "pnpm",
    "appWorkingDirectory": "npm-shim"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*": "secretlint"
  }
}
